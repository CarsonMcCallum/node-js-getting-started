<section class="game-screen hidden" id="game" data-section="game">

<div class="game-background"></div>
    <div class="deck-holder">

    </div>
<div class="d-flex flex-column flex-justify-start flex-items-center game-screen-container">
    <div class="game-top-bar"></div>
    <div class="frame flex-items-end">
      
        <div class="game-limiter">
            
        </div>
    </div>
    <div class="board-container">
        <div class="board">
           
          </div>
    </div>
    <div class="frame flex-items-center ">
       
    </div>

    <div class="player-gui" data-player-index="0">
        <img class="player-gui-bg-one" src="/images/player-gui-bg-large.png"/>
        <img class="player-gui-bg-two" src="/images/player-gui-boost-bg.png"/>
        <div class="player-gui-picture"></div>
        <div class="player-gui-details">
            <div class="player-gui-name">You</div>
            <div class="player-gui-boost-container">
                <div class="player-gui-boost-bg">
                    <div class="player-gui-boost-bar"></div>
                </div>
                <div class="player-gui-boost-x">x 1</div>
            </div>
        </div>  
    </div>
    <div class="player-gui" data-player-index="1">
        <div class="player-gui-picture"></div>
        <div class="player-gui-details">
            <div class="player-gui-name">Player 2</div>
            <div class="player-gui-boost-container">
                <div class="player-gui-boost-bg">
                    <div class="player-gui-boost-bar"></div>
                </div>
                <div class="player-gui-boost-x">x 1</div>
            </div>
        </div>
    </div>
    <div class="player-sidebar">
     
    </div>
    <div class="time-box">
        <div class="time">--:--</div>
    </div>
    <div class="score-box two-player small">
        <div class="score-box-inner">
          <div class="scoreboard-value" data-player="one">0</div> 
          <div class="scoreboard-value" data-player="two">0</div> 
        </div>
    </div>

    <div class="player-bar" data-player="one">
        <div class="player-bar-inner">
            <div class="player-bar-background"></div>
            <div class="player-bar-image"></div>
        </div>
    </div>

    <div class="player-bar" data-player="two">
        <div class="player-bar-inner">
            <div class="player-bar-background"></div>
            <div class="player-bar-image"></div>
        </div>
    </div>

</div>

<div class="game-effects">
    <div class="game-effects-inner">
        
    </div>
</div>

    <div class="shout-effect hidden">
        <div class="shout-effect-background"></div>
        <div class="shout-effect-text">Start!</div>
    </div>

    <div class="shout hidden">
        <div class="shout-backgrounds">
            <div class="shout-backgrounds-inner">
                 <div class="shout-background-matches">
                    <img class="beams" src="/images/beams.png"/>

                 </div>
             </div>
        </div>
        <div class="shout-inner">
            <div class="shout-text">
                Match!
              </div>
            <div class="matches-container hidden">
              
                <div class="matches">
                    <div class="matches-box" data-index="0"></div>
                    <div class="matches-box"  data-index="1"></div>
                    <div class="matches-box"  data-index="2"></div>
                </div>
            </div>
        </div>
    </div>

    <div class="dev-button one" onClick="randomCorrectMatch();">CM</div>

</section>